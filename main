#calculadora universal
def dolares_a_pesos(dolares):
    pesos = dolares * 19.41
    return pesos

def costo_hotel(noches):
    noc = noches * 185
    return noc

def costo_avion(pasajeros):
    pas = pasajeros * 410
    return pas

def costo_viaje(noches,pasajeros):
    a = noches * 185
    b = pasajeros * 410
    c = a*19.41 + b*19.41
    d = c
    return d

opcion = int(input())     
if(opcion == 1):
    dolares = float(input())
    print(dolares_a_pesos(dolares))
elif(opcion == 2):
    noches = int(input())
    print(costo_hotel(noches))
elif(opcion == 3):
    pasajeros = int(input())
    print(costo_avion(pasajeros))
elif(opcion == 4):
    noches = int(input())
    pasajeros = int(input())
    print(costo_viaje(noches,pasajeros))
